<!DOCTYPE html>
<html>
    <head>
        <title>PersonAccess</title>
        <!--Import Google Icon Font-->
        <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
        <!--Import materialize.css-->
        <!--<link type="text/css" rel="stylesheet" href="css/materialize.min.css"  media="screen,projection"/>-->

        <!--Let browser know website is optimized for mobile-->
        <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
        <%= csrf_meta_tags %>
        <%= csp_meta_tag %>

        <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
        <%= javascript_include_tag 'application', "rails.validations" %>

    </head>

    <body>
        <div class="container">
         <% if session[:usuario_id] %>
        <nav>
            <div class="nav-wrapper">
                 <a href="#" class="brand-logo">Logo</a>
                 <!--<a href="#!" class="brand-logo"><i class="material-icons">cloud</i>Logo</a>-->
                <ul class="right hide-on-med-and-down">
                    <li><a href="<%= usuario_url  %>" class="btn tooltipped" data-tooltip="Gestion Usuarios"><i class="material-icons">account_circle</i></a></li>
                    <li><a href="<%= organizacion_url  %>" class="btn tooltipped" data-tooltip="Dependencia"><i class="material-icons">account_balance</i></a></li>
                    <li><a href="<%= acceso_path %>" class="btn tooltipped" data-tooltip="Acceso Empresarial"><i class="material-icons">import_export</i></a></li>
                    <li><a href="<%= persona_path %>" class="btn tooltipped" data-tooltip="Personas"><i class="material-icons">people</i></a></li>
                     <li><a class="material-icons" data-position="bottom" data-tooltip="cerrar sesion" href="<%= cerrarSesion_path %>" ><i class="material-icons">exit_to_app</i></a></li>
                </ul>
            </div>
        </nav>
         <% end %>
        <!-- Page Content goes here -->
        <%= yield %>
        </div>
    </body>
</html>
