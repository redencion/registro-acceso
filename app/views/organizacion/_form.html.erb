<h1>Organizacion#_form</h1>

<%= form_for (@org), url:organizacion_path do |f| %>
    <% if @org.errors.any? %>
        <div id="error_explanation">
            <h2><%= pluralize(@org.errors.count, "error" ) %> fall√≥:</h2>
            <ul>
                <% @org.errors.full_messages.each do |message| %>
                    <li> <%= message %></li>
                <%end%>
            </ul>
        </div>
    <% end %>
<div class="field">
    <%= f.label :nombre%><br>
    <%= f.text_field :nombre%><br>
</div>
<div class="actions">
    <%= f.submit %>
</div>
<% end %>
