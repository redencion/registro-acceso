<p id="notice"> <%= notice %></p>

<h1>Listando Informacion </h1>

<table>
    <thead>
        <tr>
            <th>Nombre</th>
            <th>Fecha registro</th>
        </tr>
    </thead>
    <tbody>
        <% @organizacion.each do |org| %>
          <tr>
            <td><%= org.nombre %></td>
            <td><%= org.fecha_registro %></td>
            <td><%= link_to 'mostrar', organizacion_mostrar_path(org) %></td>
            <td><%= link_to 'editar', organizacion_editar_path(org) %></td>
             <td><%= link_to 'Eliminar', organizacion_eliminar_url(org), method: :delete, data: { confirm: 'Esta seguro de eliminar esta organizacion!?' } %></td>
          </tr>
        <%end%>
    </tbody>
</table>
<br>
<%= link_to 'Registrar Organizacion', organizacion_nueva_path %>



